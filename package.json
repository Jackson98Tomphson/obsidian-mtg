	"license": "MIT",
	"devDependencies": {
		"@types/expect": "^24.3.0",
		"@types/mocha": "^9.1.1",
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"chai": "^4.3.6",
		"esbuild": "0.14.47",
		"eslint": "^8.24.0",
		"husky": "^8.0.1",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "4.7.4"
	},
	"dependencies": {
		"@testdeck/mocha": "^0.2.1",
		"csv": "^6.2.0",
		"mocha": "^10.0.0",
